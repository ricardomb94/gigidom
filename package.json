{
  "name": "backend",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "yarn --cwd ./frontend run start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "heroku-postbuild":"YARN_CONFIG_PRODUTION=false yarn install --prefix frontend && yarn build --prefix frontend"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.15.0",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.21"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "material-ui-flags": "^1.2.4"
  }
}
